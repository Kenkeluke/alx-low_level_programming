 #include "main.h"

/**
 * print_alphabet_x10- entry point
 * Description :This function prints 10 lines of lowercase alphabets with nested 
 *loops
 *
 *
 * returns void, always success
 *
 *
 */



void print_alphabet_x10(void)
{
int a;
int b;
for(b = 0; b < 10; b++)
{

for (a = 97; a < 123; a++)
_putchar(a);
_putchar('\n');
}
}
